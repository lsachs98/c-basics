#include "list.h"

 void append(Node toAppend, Node* head){

   if(head != NULL && toAppend != NULL && *head != NULL){

     Node prevNode = *head;
     Node nextNode = *head;

     while(next != NULL){

       prevNode = next;
       nextNode = nextNode->next;

     }
        
     if(prevNode != NULL)
       prevNode->next = toAppend;
     else
       (*head)->next = toAppend;

   }
}
