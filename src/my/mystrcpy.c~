#include "../../include/my.h"

char* my_strcpy(char* dst, char* src)
{

  if(dst == NULL)
    return NULL;

  if(src == NULL)
    return NULL;

  if(strlen(dst) > strlen(src))
    {
      int srclen = my_strlen(src);

      for(int i = 0; i < srclen; i++)
	dst[i] = scr[i];

      dst[srclen] = '\0';
    }

  return dst;

}
